{
  "name": "vsn",
  "version": "1.0.6",
  "description": "SEO Friendly Javascript/Typescript Framework",
  "keywords": [
    "framework",
    "typescript",
    "html",
    "ajax",
    "hateoas",
    "rest",
    "seo",
    "reactive",
    "component",
    "web component",
    "web components",
    "web component framework"
  ],
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.17",
    "jsdom": "^27.0.1",
    "tsup": "^8.2.0",
    "typescript": "^5.6.3",
    "vitest": "^4.0.17"
  },
  "engines": {
    "node": ">=18.18.0"
  }
}
