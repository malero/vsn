{
  "name": "visionjs-framework",
  "version": "0.0.1",
  "description": "SEO Friendly Javascript/Typescript Framework",
  "keywords": [
    "framework",
    "typescript"
  ],
  "main": "./dist/Vision.js",
  "scripts": {
    "build": "rm -rf ./dist/ && tsc",
    "demo": "rm -rf ./dist/ && tsc && browserify ./dist/Vision.js > ./demo/vision.js",
    "clean": "rm -rf ./dist/",
    "test": "karma start --single-run",
    "posttest": "codecov",
    "sample": "rm -rf sample/dist/ && tsc -p sample/ && node sample/dist/sample/index.js"
  },
  "license": "MIT",
  "dependencies": {
    "simple-ts-event-dispatcher": "^0.1.5",
    "simple-ts-models": "^0.1.2",
    "simple-ts-promise": "^0.1.4"
  },
  "devDependencies": {
    "@types/jasmine": "^2.6.0",
    "codecov": "^3.7.1",
    "jasmine": "^2.8.0",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-typescript": "^3.0.5",
    "nyc": "^11.5.0",
    "typescript": "^2.5.2"
  },
  "homepage": "https://www.visionjs.io",
  "git": "https://github.com/malero/vision",
  "typings": "./dist/Vision.d.ts",
  "author": "Malero <contact@maleero.com> (http://www.maleero.com/)",
  "bugs": {
    "url": "https://github.com/malero/vision/issues",
    "email": "contact@maleero.com"
  }
}
